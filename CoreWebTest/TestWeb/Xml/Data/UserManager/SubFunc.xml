<?xml version="1.0" encoding="UTF-8"?>
<tk:Toolkit version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\DataXml.xsd" xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tk:Table TableName="SYS_SUB_FUNC">
    <tk:TableDesc>
      <tk:Content>附属功能</tk:Content>
    </tk:TableDesc>
    <tk:Field DataType="int" IsKey="true" IsEmpty="false">
      <tk:FieldName>SF_ID</tk:FieldName>
      <tk:NickName>Id</tk:NickName>
      <tk:DisplayName>
        <tk:Content>功能ID</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Hidden" Order="10" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="int">
      <tk:FieldName>SF_FN_ID</tk:FieldName>
      <tk:NickName>FnId</tk:NickName>
      <tk:DisplayName>
        <tk:Content>所属菜单功能</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Hidden" Order="20" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>SF_NAME_ID</tk:FieldName>
      <tk:NickName>NameId</tk:NickName>
      <tk:DisplayName>
        <tk:Content>子功能ID</tk:Content>
      </tk:DisplayName>
      <tk:Length>50</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="30" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>SF_NAME</tk:FieldName>
      <tk:NickName>Name</tk:NickName>
      <tk:DisplayName>
        <tk:Content>子功能名称</tk:Content>
      </tk:DisplayName>
      <tk:Length>50</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="40" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>SF_POSITION</tk:FieldName>
      <tk:NickName>Position</tk:NickName>
      <tk:DisplayName>
        <tk:Content>位置</tk:Content>
      </tk:DisplayName>
      <tk:Length>10</tk:Length>
      <tk:CodeTable RegName="OperatorPosition"/>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Combo" Order="50" DefaultShow="All"/>
      <tk:Edit>
        <tk:DefaultValue>Row</tk:DefaultValue>
      </tk:Edit>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>SF_ICON</tk:FieldName>
      <tk:NickName>Icon</tk:NickName>
      <tk:DisplayName>
        <tk:Content>图标类</tk:Content>
      </tk:DisplayName>
      <tk:Length>255</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="60" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="short">
      <tk:FieldName>SF_USE_KEY</tk:FieldName>
      <tk:NickName>UseKey</tk:NickName>
      <tk:DisplayName>
        <tk:Content>使用Key</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="CheckBox" Order="70" DefaultShow="All"/>
      <tk:Extension CheckValue="1" UnCheckValue="0"/>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>SF_CONTENT</tk:FieldName>
      <tk:NickName>Content</tk:NickName>
      <tk:DisplayName>
        <tk:Content>地址</tk:Content>
      </tk:DisplayName>
      <tk:Length>255</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="80" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="short">
      <tk:FieldName>SF_USE_MARCO</tk:FieldName>
      <tk:NickName>UseMarco</tk:NickName>
      <tk:DisplayName>
        <tk:Content>地址包含宏</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="CheckBox" Order="90" DefaultShow="All"/>
      <tk:Edit>
        <tk:DefaultValue>0</tk:DefaultValue>
      </tk:Edit>
      <tk:Extension CheckValue="1" UnCheckValue="0"/>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>SF_CONFIRM_DATA</tk:FieldName>
      <tk:NickName>ConfirmData</tk:NickName>
      <tk:DisplayName>
        <tk:Content>确认对话信息</tk:Content>
      </tk:DisplayName>
      <tk:Length>255</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="100" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>SF_DIALOG_TITLE</tk:FieldName>
      <tk:NickName>DialogTitle</tk:NickName>
      <tk:DisplayName>
        <tk:Content>对话框标题</tk:Content>
      </tk:DisplayName>
      <tk:Length>255</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="110" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>SF_INFO</tk:FieldName>
      <tk:NickName>Info</tk:NickName>
      <tk:DisplayName>
        <tk:Content>附加信息</tk:Content>
      </tk:DisplayName>
      <tk:Length>255</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="120" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="int" IsEmpty="false">
      <tk:FieldName>SF_ORDER</tk:FieldName>
      <tk:NickName>OperOrder</tk:NickName>
      <tk:DisplayName>
        <tk:Content>排序</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="125" DefaultShow="All"/>
      <tk:ListDetail ListWidth="60" />
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>SF_PAGE</tk:FieldName>
      <tk:NickName>Page</tk:NickName>
      <tk:DisplayName>
        <tk:Content>所在页面</tk:Content>
      </tk:DisplayName>
      <tk:Length>10</tk:Length>
      <tk:CodeTable RegName="OperatorPage"/>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Combo" Order="130" DefaultShow="All"/>
    </tk:Field>
  </tk:Table>
</tk:Toolkit>
