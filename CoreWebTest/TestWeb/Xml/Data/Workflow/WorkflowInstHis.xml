<?xml version="1.0" encoding="UTF-8"?>
<tk:Toolkit xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\DataXml.xsd">
  <tk:Table TableName="WF_WORKFLOW_INST_HIS">
    <tk:TableDesc>
      <tk:Content>工作流实例历史</tk:Content>
    </tk:TableDesc>
    <tk:Field DataType="int" IsKey="true">
      <tk:FieldName>WI_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>流程ID</tk:Content>
      </tk:DisplayName>
      <tk:Control Control="Hidden" Order="10" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="true">
      <tk:FieldName>WI_WD_NAME</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>流程模式名</tk:Content>
      </tk:DisplayName>
      <tk:Length>50</tk:Length>
      <tk:EasySearch RegName="WorkFlowDefine"/>
      <tk:Control Control="EasySearch" Order="20" DefaultShow="All"/>
      <tk:ListDetail Search="true"/>
    </tk:Field>
    <tk:Field DataType="text" IsEmpty="true">
      <tk:FieldName>WI_CONTENT_XML</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>流程内容</tk:Content>
      </tk:DisplayName>
      <tk:Control Control="TextArea" Order="30" DefaultShow="None"/>
    </tk:Field>
    <tk:Field DataType="string">
