      <tk:Control Control="EasySearch" Order="60" DefaultShow="All"/>
      <tk:ListDetail Search="true"/>
    </tk:Field>
    <tk:Field DataType="DateTime" IsEmpty="true">
      <tk:FieldName>WI_END_DATE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>结束日期</tk:Content>
      </tk:DisplayName>
      <tk:Control Control="Text" Order="70" DefaultShow="All"/>
    </tk:Field>
