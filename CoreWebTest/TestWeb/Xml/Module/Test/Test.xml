<?xml version="1.0" encoding="UTF-8"?>
<tk:Toolkit version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\Module.xsd" xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tk:Module>
    <tk:Title>
      <tk:Content>学生表</tk:Content>
    </tk:Title>
    <tk:MetaData>
      <tk:MasterDetailMetaData>
        <tk:Master>
          <tk:SingleXmlMetaData DataXml="Student/Student.xml"/>
        </tk:Master>
        <tk:Detail ListStyle="Table">
          <tk:SingleXmlMetaData DataXml="Student/School.xml"/>
        </tk:Detail>
      </tk:MasterDetailMetaData>
    </tk:MetaData>
    <tk:Source>
      <tk:MasterDetailDbSource>
        <tk:MasterResolver>
          <tk:MetaDataResolver AutoTrackField="true" AutoUpdateKey="true">
            <tk:Scheme>
              <tk:Tk5DataXml FileName="Student/Student.xml"/>
            </tk:Scheme>
          </tk:MetaDataResolver>
        </tk:MasterResolver>
        <tk:Detail>
          <tk:Resolver>
            <tk:MetaDataResolver AutoTrackField="true" AutoUpdateKey="true">
              <tk:Scheme>
                <tk:Tk5DataXml FileName="Student/School.xml"/>
              </tk:Scheme>
            </tk:MetaDataResolver>
          </tk:Resolver>
          <tk:Relation>
            <tk:MasterFields>school_id</tk:MasterFields>
            <tk:DetailFields>school_id</tk:DetailFields>
          </tk:Relation>
        </tk:Detail>
      </tk:MasterDetailDbSource>
    </tk:Source>
    <tk:PageMaker>
      <tk:RazorModuleTemplatePageMaker ModuleTemplate="MasterDetail"/>
    </tk:PageMaker>
  </tk:Module>
</tk:Toolkit>
