              <tk:Scheme>
                <tk:Tk5DataXml FileName="WorkOrder/WorkOrdersAttachment.xml"/>
              </tk:Scheme>
            </tk:MetaDataResolver>
          </tk:Resolver>
          <tk:Relation>
            <tk:MasterFields>Id</tk:MasterFields>
            <tk:DetailFields>WoId</tk:DetailFields>
          </tk:Relation>
        </tk:Detail>
        <tk:TabSheets>
          <tk:TabSheet Id="DPQ">
            <tk:Caption>
              <tk:Content>未确认</tk:Content>
            </tk:Caption>
            <tk:Condition>WO_STATUS=1</tk:Condition>
