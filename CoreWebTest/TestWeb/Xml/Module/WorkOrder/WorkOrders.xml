      </tk:MasterDetailMetaData>
    </tk:MetaData>
    <tk:Source>
      <tk:MasterDetailDbSource>
        <tk:MasterResolver>
          <tk:MetaDataResolver AutoTrackField="true" AutoUpdateKey="true">
            <tk:Scheme>
              <tk:Tk5DataXml FileName="WorkOrder/WorkOrders.xml"/>
            </tk:Scheme>
          </tk:MetaDataResolver>
        </tk:MasterResolver>
        <tk:Detail>
          <tk:Resolver>
