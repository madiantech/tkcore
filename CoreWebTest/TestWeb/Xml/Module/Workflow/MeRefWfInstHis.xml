<?xml version="1.0" encoding="UTF-8"?>
<tk:Toolkit version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\Module.xsd" xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tk:Module>
		<tk:Title>
			<tk:Content>我参与的历史工作流</tk:Content>
		</tk:Title>
		<tk:MetaData>
			<tk:SingleXmlMetaData DataXml="Workflow/WorkflowInstHis.xml" DisableAutoDetailLink="true">
				<tk:Override>
					<tk:Field NickName="Name">
						<tk:ListDetail>
							<tk:ListDisplay>
								<tk:LinkDisplay>
									<tk:NormalDisplay/>
									<tk:DialogHrefDisplay Mode="Dialog">~/c/~source/C/WfViewContent?WfId=*Id*&amp;view=history</tk:DialogHrefDisplay>
								</tk:LinkDisplay>
							</tk:ListDisplay>
						</tk:ListDetail>
					</tk:Field>
				</tk:Override>
			</tk:SingleXmlMetaData>
		</tk:MetaData>
		<tk:Source>
			<tk:DbListSource OrderBy="ORDER BY WI_ID DESC">
				<tk:Resolver>
					<tk:RegResolver>WorkflowInstHisWeb</tk:RegResolver>
				</tk:Resolver>
				<tk:FilterSql NeedParse="true">WI_REF_LIST LIKE '%"{UserId}"%'</tk:FilterSql>
				<tk:Operators>
					<tk:EmptyOperators/>
				</tk:Operators>
			</tk:DbListSource>
		</tk:Source>
		<tk:PageMaker>
			<tk:RazorModuleTemplatePageMaker ModuleTemplate="Single">
				<tk:PageTemplate Style="List">
					<tk:RazorData>
						<tk:NormalListData DialogHeight="700" OperatorPosition="None"/>
					</tk:RazorData>
					<tk:Script>usercss/workflow/dialogwidth.css</tk:Script>
				</tk:PageTemplate>
			</tk:RazorModuleTemplatePageMaker>
		</tk:PageMaker>
	</tk:Module>
</tk:Toolkit>
