@inherits YJC.Toolkit.Razor.TemplatePage<TModel>
@using System.Data;
@using YJC.Toolkit.MetaData;
@using YJC.Toolkit.Sys;
@using YJC.Toolkit.Razor;
@using YJC.Toolkit.Data;
@{
    dynamic info = Model;
    string url = string.Format("/js/{0}/list/{1}?Model={2}&Template={3}",
        info.ModuleCreator, info.Source, info.Model, info.Template);
    if (info.Id != null)
    {
        url += string.Format("&Id={0}&_={1}", info.Id, info.Ticks);
    }
}
<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type"
          content="text/html; charset=utf-8" />
    <title>增删改查</title>
</head>

<body data-json='{"dataUrl":"/c/xml/list/Test/TestPartVueList?Type=Data","tableName":"UR_PART"}'>
    <div id='app'></div>
    <script type="text/javascript" src="@url"></script>
</body>
</html>