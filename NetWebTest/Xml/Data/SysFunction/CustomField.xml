      <tk:DisplayName>
        <tk:Content>控件</tk:Content>
      </tk:DisplayName>
      <tk:Length>50</tk:Length>
      <tk:CodeTable RegName="CD_TK_CONTROL"/>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Combo" Order="70" DefaultShow="All"/>
      <!--<tk:ListDetail ListWidth="200"/>-->
    </tk:Field>
    <tk:Field DataType="short">
      <tk:FieldName>CF_CHARACTER</tk:FieldName>
