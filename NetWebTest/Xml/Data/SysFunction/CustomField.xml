      </tk:DisplayName>
      <tk:Length>255</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Hidden" Order="120" DefaultShow="None"/>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>CF_EXTIONSION3</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>字段扩展3</tk:Content>
      </tk:DisplayName>
      <tk:Length>255</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Hidden" Order="130" DefaultShow="None"/>
    </tk:Field>
