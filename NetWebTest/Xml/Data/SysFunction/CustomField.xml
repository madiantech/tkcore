<?xml version="1.0" encoding="UTF-8"?>
<tk:Toolkit version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\DataXml.xsd" xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tk:Table TableName="SYS_CUSTOM_FIELD">
    <tk:TableDesc>
      <tk:Content>自定义字段</tk:Content>
    </tk:TableDesc>
    <tk:Field DataType="int" IsKey="true" IsEmpty="false">
      <tk:FieldName>CF_TABLE_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>主表Id</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Hidden" Order="10" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string" IsKey="true" IsEmpty="false">
      <tk:FieldName>CT_FIELD_NAME</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>字段名</tk:Content>
      </tk:DisplayName>
      <tk:Length>255</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Hidden" Order="20" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string">
