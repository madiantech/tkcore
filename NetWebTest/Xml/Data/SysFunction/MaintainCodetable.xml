<?xml version="1.0" encoding="UTF-8"?>
<tk:Toolkit version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\DataXml.xsd" xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tk:Table TableName="SYS_MAINTAIN_CODETABLE">
    <tk:TableDesc>
      <tk:Content>代码表维护</tk:Content>
    </tk:TableDesc>
    <tk:Field DataType="int" IsKey="true" IsEmpty="false" IsAutoInc="true">
      <tk:FieldName>MC_ID</tk:FieldName>
      <tk:NickName>Id</tk:NickName>
      <tk:DisplayName>
        <tk:Content>ID</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Hidden" Order="10" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>MC_TABLE_NAME</tk:FieldName>
      <tk:NickName>TableName</tk:NickName>
      <tk:DisplayName>
        <tk:Content>代码表名</tk:Content>
      </tk:DisplayName>
      <tk:Length>80</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="20" DefaultShow="All"/>
      <tk:Edit>
        <tk:DefaultValue>CD_</tk:DefaultValue>
      </tk:Edit>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>MC_DESCRIPTION</tk:FieldName>
      <tk:NickName>Description</tk:NickName>
      <tk:DisplayName>
        <tk:Content>说明</tk:Content>
      </tk:DisplayName>
      <tk:Length>255</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="30" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>MC_PINYIN_CAPTION</tk:FieldName>
      <tk:NickName>PinyinCaption</tk:NickName>
      <tk:DisplayName>
        <tk:Content>拼音标题</tk:Content>
      </tk:DisplayName>
      <tk:Length>80</tk:Length>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="40" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="short">
      <tk:FieldName>MC_IS_VALUE</tk:FieldName>
      <tk:NickName>IsValue</tk:NickName>
      <tk:DisplayName>
        <tk:Content>是否录入代码</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="CheckBox" Order="50" DefaultShow="All"/>
      <tk:Edit>
        <tk:DefaultValue>0</tk:DefaultValue>
      </tk:Edit>
      <tk:Extension CheckValue="1" UnCheckValue="0"/>
    </tk:Field>
    <tk:Field DataType="short">
      <tk:FieldName>MC_IS_SORT</tk:FieldName>
      <tk:NickName>IsSort</tk:NickName>
      <tk:DisplayName>
        <tk:Content>是否录入排序</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="CheckBox" Order="60" DefaultShow="All"/>
      <tk:Edit>
        <tk:DefaultValue>0</tk:DefaultValue>
      </tk:Edit>
      <tk:Extension CheckValue="1" UnCheckValue="0"/>
    </tk:Field>
    <tk:Field DataType="short">
      <tk:FieldName>MC_IS_PINYIN</tk:FieldName>
      <tk:NickName>IsPinyin</tk:NickName>
      <tk:DisplayName>
        <tk:Content>是否录入拼音</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="CheckBox" Order="70" DefaultShow="All"/>
      <tk:Edit>
        <tk:DefaultValue>0</tk:DefaultValue>
      </tk:Edit>
      <tk:Extension CheckValue="1" UnCheckValue="0"/>
    </tk:Field>
    <tk:Field DataType="short">
      <tk:FieldName>MC_IS_AUTO_KEY</tk:FieldName>
      <tk:NickName>IsAutoKey</tk:NickName>
      <tk:DisplayName>
        <tk:Content>自动生成代码</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="CheckBox" Order="80" DefaultShow="All"/>
      <tk:Edit>
        <tk:DefaultValue>1</tk:DefaultValue>
      </tk:Edit>
      <tk:Extension CheckValue="1" UnCheckValue="0"/>
    </tk:Field>
    <tk:Field DataType="short">
      <tk:FieldName>MC_AUTO_PINYIN</tk:FieldName>
      <tk:NickName>AutoPinyin</tk:NickName>
      <tk:DisplayName>
        <tk:Content>拼音自动生成</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="CheckBox" Order="90" DefaultShow="All"/>
      <tk:Edit>
        <tk:DefaultValue>1</tk:DefaultValue>
      </tk:Edit>
      <tk:Extension CheckValue="1" UnCheckValue="0"/>
    </tk:Field>
    <tk:Field DataType="int">
      <tk:FieldName>MC_CODE_LENGTH</tk:FieldName>
      <tk:NickName>CodeLength</tk:NickName>
      <tk:DisplayName>
        <tk:Content>代码长度</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="100" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="int">
      <tk:FieldName>MC_DIALOG_HEIGHT</tk:FieldName>
      <tk:NickName>DialogHeight</tk:NickName>
      <tk:DisplayName>
        <tk:Content>对话框高度</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1"/>
      <tk:Control Control="Text" Order="110" DefaultShow="All"/>
      <tk:Edit>
        <tk:DefaultValue>200</tk:DefaultValue>
      </tk:Edit>
    </tk:Field>
  </tk:Table>
</tk:Toolkit>
