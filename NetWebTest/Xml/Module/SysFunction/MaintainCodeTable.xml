<tk:Toolkit version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\Module.xsd" xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tk:Module>
    <tk:Title>
      <tk:Content>代码表数据维护</tk:Content>
    </tk:Title>
    <tk:MetaData>
      <tk:SingleXmlMetaData DataXml="SysFunction/MaintainCodetable.xml" DisableAutoDetailLink="true">
        <tk:Except>
          <tk:DelField NickName="PinyinCaption"/>
          <tk:DelField NickName="IsValue"/>
          <tk:DelField NickName="IsSort"/>
          <tk:DelField NickName="IsPinyin"/>
          <tk:DelField NickName="IsAutoKey"/>
          <tk:DelField NickName="CodeLength"/>
          <tk:DelField NickName="AutoPinyin"/>
          <tk:DelField NickName="DialogHeight"/>
        </tk:Except>
      </tk:SingleXmlMetaData>
    </tk:MetaData>
    <tk:Source>
      <tk:SingleDbSource>
        <tk:Resolver>
          <tk:RegResolver>MaintainCodetable</tk:RegResolver>
        </tk:Resolver>
        <tk:Operators>
          <tk:StandardOperators>
            <tk:Right>
              <tk:EmptyOperateRight/>
            </tk:Right>
            <tk:Operator Id="Maintain" Info="_blank">
              <tk:Caption>
                <tk:Content>维护数据</tk:Content>
              </tk:Caption>
              <tk:Content>~/codetable/list/*TableName*.c</tk:Content>
            </tk:Operator>
          </tk:StandardOperators>
        </tk:Operators>
        <tk:DetailOperators>
          <tk:StandardOperators>
            <tk:Right>
              <tk:FieldOperateRight FieldName="IsDel">
                <tk:Empty>Insert,Pinyin</tk:Empty>
                <tk:Item Value="0" ContainsNull="true">Update,Delete,FakeDelete</tk:Item>
                <tk:Item Value="1">Restore</tk:Item>
              </tk:FieldOperateRight>
            </tk:Right>
            <tk:Operator IconClass="icon-plus" Id="Insert" Info="Dialog" Position="Global">
              <tk:Caption>
                <tk:Content>新建</tk:Content>
              </tk:Caption>
              <tk:Content NeedParse="true">~/{ModuleCreator}/insert/{CcSource}.c</tk:Content>
            </tk:Operator>
            <tk:Operator IconClass="icon-linkedin" Id="Pinyin" Info="AjaxUrl" Position="Global">
              <tk:Caption>
                <tk:Content>生成拼音</tk:Content>
              </tk:Caption>
              <tk:Content NeedParse="true">~/{ModuleCreator}/CPinyin/{CcSource}.c</tk:Content>
              <tk:ConfirmData>
                <tk:Content>确认生成拼音吗？</tk:Content>
              </tk:ConfirmData>
            </tk:Operator>
            <tk:Operator IconClass="icon-edit" Id="Update" Info="Dialog" Position="Row" UseKey="true">
              <tk:Caption>
                <tk:Content>修改</tk:Content>
              </tk:Caption>
              <tk:Content NeedParse="true">~/{ModuleCreator}/update/{CcSource}.c</tk:Content>
            </tk:Operator>
            <tk:Operator Id="Delete" Info="AjaxUrl" Position="Row" UseKey="true">
              <tk:Caption>
                <tk:Content>删除</tk:Content>
              </tk:Caption>
              <tk:Content NeedParse="true">~/{ModuleCreator}/delete/{CcSource}.c</tk:Content>
              <tk:ConfirmData>
                <tk:Content>确认删除吗？</tk:Content>
              </tk:ConfirmData>
            </tk:Operator>
            <tk:Operator Id="FakeDelete" Info="AjaxUrl" Position="Row" UseKey="true">
              <tk:Caption>
                <tk:Content>逻辑删除</tk:Content>
              </tk:Caption>
              <tk:Content NeedParse="true">~/{ModuleCreator}/CFakeDelete/{CcSource}.c</tk:Content>
            </tk:Operator>
            <tk:Operator Id="Restore" Info="AjaxUrl" Position="Row">
              <tk:Caption>
                <tk:Content>逻辑恢复</tk:Content>
              </tk:Caption>
              <tk:Content NeedParse="true">~/{ModuleCreator}/CRestore/{CcSource}.c</tk:Content>
            </tk:Operator>
          </tk:StandardOperators>
        </tk:DetailOperators>
      </tk:SingleDbSource>
    </tk:Source>
    <tk:PageMaker>
      <tk:SingleRazorPageMaker ListDisplay="Striped">
        <tk:Item Style="List">
          <tk:RazorData>
            <tk:NormalListData ShowExportExcel="false"/>
          </tk:RazorData>
        </tk:Item>
      </tk:SingleRazorPageMaker>
    </tk:PageMaker>
  </tk:Module>
</tk:Toolkit>
