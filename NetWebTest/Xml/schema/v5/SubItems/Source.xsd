        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="UseCallerInfo" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>是否在结果集中添加附加信息</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attributeGroup ref="tk:__tk_ContextGroup"/>
    </xs:complexType>
