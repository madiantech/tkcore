    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Tree" type="tk:TreeConfigType"/>
      </xs:sequence>
      <xs:attributeGroup ref="tk:__tk_ContextGroup"/>
    </xs:complexType>
  </xs:element>
  <!--多表Source类型-->
