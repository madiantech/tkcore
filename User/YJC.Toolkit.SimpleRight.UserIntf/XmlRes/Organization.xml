<?xml version="1.0" encoding="utf-8"?>
<tk:Toolkit xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\DataXml.xsd">
  <tk:Table TableName="SYS_ORGANIZATION">
    <tk:TableDesc>
      <tk:Content>组织机构表</tk:Content>
    </tk:TableDesc>
    <tk:Field DataType="string" IsKey="true">
      <tk:FieldName>ORG_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>机构ID</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>ORG_NAME</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>机构名称</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>ORG_LAYER</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>机构LAYER</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>ORG_CODE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>机构代码</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>ORG_PARENT_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>上级机构</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="int" IsEmpty="true">
      <tk:FieldName>ORG_IS_LEAF</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>是否是叶节点</tk:Content>
      </tk:DisplayName>
    </tk:Field>
  </tk:Table>
</tk:Toolkit>