<?xml version="1.0" encoding="utf-8"?>
<tk:Toolkit xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\DataXml.xsd">
  <tk:Table TableName="UR_USERS">
    <tk:TableDesc>
      <tk:Content>用户信息</tk:Content>
    </tk:TableDesc>
    <tk:Field DataType="string" IsKey="true">
      <tk:FieldName>USER_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>ID</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>USER_NAME</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>用户姓名</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>USER_ORG_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>所属机构</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>USER_LOGIN_NAME</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>登录帐号</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="true">
      <tk:FieldName>USER_LOGIN_PASSWD</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>登陆密码</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="Date" IsEmpty="true">
      <tk:FieldName>USER_LOGIN_DATE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>上次登入时间</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="true">
      <tk:FieldName>USER_PHONE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>联系电话</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="true">
      <tk:FieldName>USER_EMAIL</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>E-mail</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="true">
      <tk:FieldName>USER_WORK_NO</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>工号</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="int" IsEmpty="true">
      <tk:FieldName>USER_ACTIVE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>有效用户</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="DateTime" IsEmpty="true">
      <tk:FieldName>USER_CREATE_DATE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>创建日期</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="DateTime" IsEmpty="true">
      <tk:FieldName>USER_PASSWD_CHANGE_DATE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>最后一次修改密码时间</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="DateTime" IsEmpty="true">
      <tk:FieldName>USER_UNLOCK_DATE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>自动解封时间</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="int" IsEmpty="true">
      <tk:FieldName>USER_UPDATE_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>修改人</tk:Content>
      </tk:DisplayName>
    </tk:Field>
    <tk:Field DataType="DateTime" IsEmpty="true">
      <tk:FieldName>USER_UPDATE_DATE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>修改日期</tk:Content>
      </tk:DisplayName>
    </tk:Field>
  </tk:Table>
</tk:Toolkit>