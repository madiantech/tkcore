<?xml version="1.0" encoding="UTF-8"?>
<tk:Toolkit version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\DataXml.xsd" xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tk:Table TableName="WF_APPROVE_HISTORY">
    <tk:TableDesc>
      <tk:Content>审批过程</tk:Content>
    </tk:TableDesc>
    <tk:Field DataType="int" IsKey="true">
      <tk:FieldName>AH_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>ID</tk:Content>
      </tk:DisplayName>
      <tk:Control Control="Hidden" Order="10" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="int" IsEmpty="true">
      <tk:FieldName>AH_WORKFLOW_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>工作流ID</tk:Content>
      </tk:DisplayName>
      <tk:Control Control="Text" Order="20" DefaultShow="None"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="true">
      <tk:FieldName>AH_STEP_NAME</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>步骤名称</tk:Content>
      </tk:DisplayName>
      <tk:Length>50</tk:Length>
      <tk:Control Control="Text" Order="30" DefaultShow="None"/>
      <tk:ListDetail Search="true"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="true">
      <tk:FieldName>AH_STEP_DISPLAY_NAME</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>流转环节</tk:Content>
      </tk:DisplayName>
      <tk:Length>50</tk:Length>
      <tk:Control Control="Text" Order="55" DefaultShow="List"/>
      <tk:ListDetail Search="true"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="true">
      <tk:FieldName>AH_OPERATOR</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>审核人</tk:Content>
      </tk:DisplayName>
      <tk:Length>64</tk:Length>
      <tk:EasySearch RegName="USER"/>
      <tk:Control Control="EasySearch" Order="60" DefaultShow="List"/>
      <tk:ListDetail Search="true"/>
    </tk:Field>
    <tk:Field DataType="int" IsEmpty="false">
      <tk:FieldName>AH_APPROVE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>操作</tk:Content>
      </tk:DisplayName>
      <tk:CodeTable RegName="ApproveOperator"/>
      <tk:Control Control="RadioGroup" Order="40" DefaultShow="List Update Insert"/>
      <tk:Edit>
        <tk:DefaultValue>0</tk:DefaultValue>
      </tk:Edit>
    </tk:Field>
    <tk:Field DataType="text" IsEmpty="true">
      <tk:FieldName>AH_NOTE</tk:FieldName>
      <tk:NickName>ApproveNote</tk:NickName>
      <tk:DisplayName>
        <tk:Content>流转意见</tk:Content>
      </tk:DisplayName>
      <tk:Control Control="TextArea" Order="50" DefaultShow="List Update Insert"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="true">
      <tk:FieldName>AH_CREATE_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>创建人</tk:Content>
      </tk:DisplayName>
      <tk:Length>64</tk:Length>
      <tk:EasySearch RegName="USER"/>
      <tk:Control Control="EasySearch" Order="80" DefaultShow="None"/>
      <tk:ListDetail Search="true"/>
    </tk:Field>
    <tk:Field DataType="DateTime" IsEmpty="true">
      <tk:FieldName>AH_CREATE_DATE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>审批时间</tk:Content>
      </tk:DisplayName>
      <tk:Control Control="Text" Order="90" DefaultShow="List"/>
      <tk:ListDetail Search="true"/>
    </tk:Field>
  </tk:Table>
</tk:Toolkit>
